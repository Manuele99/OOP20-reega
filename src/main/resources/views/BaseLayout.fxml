<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import reega.viewutils.ContentControl?>

<fx:root xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            type="ScrollPane"
            prefHeight="800.0"
            prefWidth="700.0"
            stylesheets="@/css/Common.css"
            fitToWidth="true" fitToHeight="true">
    <content>
        <StackPane>
            <children>
                <Button fx:id="backArrowButton" StackPane.alignment="TOP_LEFT" onAction="#popController"/>
                <ContentControl fx:id="contentControl" StackPane.alignment="CENTER"/>
            </children>
        </StackPane>
    </content>
</fx:root>
