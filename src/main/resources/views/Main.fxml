<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import reega.viewcomponents.*?>


<fx:root xmlns="http://javafx.com/javafx"
        xmlns:fx="http://javafx.com/fxml"
        type="HBox"
        prefHeight="400.0"
        prefWidth="600.0"
        alignment="CENTER"
        stylesheets="@/css/Card.css">
    <children>
        <VBox>
            <children>
                <Button/>
            </children>
        </VBox>
        <VBox>
            <children>
                <Label fx:id="userEmail"/>
                <HBox fx:id="contractsPane" visible="false"/>
                <FlexibleGridPane fx:id="servicesPane" VBox.vgrow="ALWAYS" fixedColumnsNumber="2"/>
            </children>

        </VBox>
    </children>
</fx:root>
